<a href="/teacherLogin" class="back-button">Back</a>
<div class="table-container">
    <h2>STUDENT'S INFORMATION</h2>
<table>
    <thead>
        <tr>
            <th>Roll No</th>
            <th>Name</th>
            <th>Score</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        <% student.forEach(element=>{ %>
            <tr>
                <td><%= element.rollno %></td>
                <td><%= element.name %></td>
                <td><%= element.score %></td>
                <td><a href="edit/<%= element._id %>" <i class="fa-solid fa-pen-to-square"></i></a></td>
                <td><a href="delete/<%= element._id %>"<i class="fa-solid fa-trash"></i></a></td>
            </tr>
        <% }) %>
    </tbody>
</table>
</div>






